@prefix ex:   <http://example.com/viessmann#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

### Classes
ex:Product     a rdfs:Class .
ex:Component   a rdfs:Class .
ex:Standard    a rdfs:Class .
ex:ProcessStep a rdfs:Class .
ex:Material    a rdfs:Class .
ex:Refrigerant a rdfs:Class .

### Subclasses
ex:Compressor       rdfs:subClassOf ex:Component .
ex:ElectricalModule rdfs:subClassOf ex:Component .
ex:WirelessModule   rdfs:subClassOf ex:Component .

### Properties
ex:hasComponent        a rdf:Property ; rdfs:domain ex:Product ;  rdfs:range ex:Component .
ex:requiresCompliance  a rdf:Property ; rdfs:domain ex:Product ;  rdfs:range ex:Standard .
ex:requiresStep        a rdf:Property ; rdfs:domain ex:Product ;  rdfs:range ex:ProcessStep .
ex:usesMaterial        a rdf:Property ; rdfs:domain ex:Component ; rdfs:range ex:Material .
ex:usesRefrigerant     a rdf:Property ; rdfs:domain ex:Component ; rdfs:range ex:Refrigerant .
ex:conformsTo          a rdf:Property ; rdfs:domain ex:Product ;  rdfs:range ex:Standard .

### Standards (with labels)
ex:FGasStandard              a ex:Standard ; rdfs:label "EU F-Gas Regulation" .
ex:ElectricalSafetyStandard  a ex:Standard ; rdfs:label "Electrical Safety Check" .
ex:RoHSStandard              a ex:Standard ; rdfs:label "RoHS" .
ex:WirelessComplianceStandard a ex:Standard ; rdfs:label "Wireless Compliance" .

### Steps (with labels)
ex:PressureTestStep     a ex:ProcessStep ; rdfs:label "Pressure Test" .
ex:LeakCheckStep        a ex:ProcessStep ; rdfs:label "Leak Check" .
ex:ElectricalSafetyTest a ex:ProcessStep ; rdfs:label "Electrical Safety Test" .
ex:WirelessTestStep     a ex:ProcessStep ; rdfs:label "Wireless Test" .

### Materials / Refrigerants
ex:LeadMaterial a ex:Material .
ex:R32          a ex:Refrigerant .

### Products
ex:ProductV1 a ex:Product ;
  ex:hasComponent ex:Comp1, ex:Board1, ex:Radio1 ;
  ex:conformsTo ex:FGasStandard .

ex:Comp1   a ex:Compressor ;      ex:usesRefrigerant ex:R32 .
ex:Board1  a ex:ElectricalModule ; ex:usesMaterial    ex:LeadMaterial .
ex:Radio1  a ex:WirelessModule .

# Another product with fewer parts
ex:ProductV2 a ex:Product ;
  ex:hasComponent ex:Comp2, ex:Board2 .

ex:Comp2  a ex:Compressor .
ex:Board2 a ex:ElectricalModule .
